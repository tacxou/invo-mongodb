{
  "name": "phalcon/invo-mongodb",
  "type": "library",
  "description": "This is a sample application for the Phalcon PHP Framework with MongoDB",
  "keywords": [
    "phalcon",
    "framework",
    "sample app",
    "invo",
    "mongodb"
  ],
  "homepage": "https://phalcon.io",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Contributors",
      "homepage": "https://github.com/phalcon/vokuro/graphs/contributors"
    }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "phalcon/incubator-mongodb",
        "version": "0.0.1",
        "source": {
          "url": "https://github.com/tacxticx88/incubator-mongodb.git",
          "type": "git",
          "reference": "feature/new-mongodb-driver"
        }
      }
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-phalcon": "^4.0.0",
    "vlucas/phpdotenv": "^4.0",
    "phalcon/incubator-mongodb": "0.0.1",
    "mongodb/mongodb": "^1.6"
  },
  "require-dev": {
    "codeception/codeception": "^3.1",
    "phalcon/ide-stubs": "^4.0.0",
    "squizlabs/php_codesniffer": "^3.5",
    "vimeo/psalm": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "InvoMongodb\\Controllers\\": "src/controllers/",
      "InvoMongodb\\Collections\\": "src/collections/",
      "InvoMongodb\\Libraries\\": "src/libraries/"
    }
  },
  "archive": {
    "exclude": [
      "CONTRIBUTING.md"
    ]
  }
}
